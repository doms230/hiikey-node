<div id="flyerDiv" class="container-fluid">
    <img class="img-responsive" alt="..." src= <%=image%>>
</div>

<div id="eventInfoDiv" class="container-fluid">


</div>

<!-- tab jaunts -->

<div  id="locationDiv" class="page-header" hidden="yes">
</div>

<div class="container-fluid">
    <ul class="nav nav-tabs">
        <li id="messages" role="presentation" class="active"><a>Messages</a></li>
