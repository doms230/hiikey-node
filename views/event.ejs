<body>

<div class="container-fluid">
    <h1><%= title %></h1>
    <h4>By <%= user%> - <small><%= description %>  </small></h4>
    <h4> Date - <small> <%= startDate %> - <%= endDate %> </small></h4>
    <a href="#" class="thumbnail">
        <img src= <%= image %> alt="...">
    </a>
    <h4>event code: <span class="label label-danger"><%=eventCode%></span> </h4>

    <ul class="nav nav-tabs">
        <li id="location" role="presentation" class="active"><a > Location</a></li>
        <li id="messages" role="presentation"><a>Messages</a></li>
        <li id="rsvps" role="presentation"><a >RSVPs</a></li>
    </ul>
</div>

<!-- Location -->

<div  id="locationDiv" class="page-header" hidden="yes">
</div>

<div id="messageDiv" class="page-header" hidden="yes">
</div>

<div id="rsvpDiv" class="page-header" hidden="yes">
</div>

<ul id="messageBarDiv" class=" navbar-fixed-bottom">
    <div class="input-group">
        <input id="usermsg" type="text" class="form-control" placeholder= "Message" value="Hey" >
      <span class="input-group-btn">
        <button id="sendmsg" class="btn btn-default" type="button">Send</button>
      </span>
    </div><!-- /input-group -->
</ul>

<div id="signinDiv" class="container" hidden="yes">

    <form class="form-signin">
        <h3 class="form-signin-heading">Sign In</h3>
        <input type="text" id="inputUsername" class="form-control" placeholder="Username" required autofocus>
        <label for="inputPassword" class="sr-only">Password</label>
        <input type="password" id="inputPassword" class="form-control" placeholder="Password" required>
        <!-- <div class="checkbox">
             <label>
                 <input type="checkbox" value="remember-me"> Remember me
             </label>
         </div> -->
         <button id="signin" class="btn btn-lg btn-default btn-block" type="submit">Sign in & RSVP</button>
     </form>

     </br>
     <button id="signup" class="btn btn-lg btn-primary btn-block" type="submit">Sign up</button>

 </div>

